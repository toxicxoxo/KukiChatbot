{
    "name": "KukiChatBot",
    "description": "Hello, I am KUKI An Intelligent ChatBot. If You Are Feeling Lonely, You can Always Come to me and Chat With Me!",
    "logo": "https://telegra.ph/file/3d3bc9ede725f2e10e921.jpg",
    "repository": "https://github.com/toxicxoxo/KukiChatbot",
    "env": {
        "BOT_TOKEN": {
            "description": "telegram bot token here",
            "required": true
        },
        "KUKI_API": {
            "description": "join @metavoidsupport",
            "required": true
        },
        "ERROR_LOG": {
            "description": "error channel log id",
            "required": true
        },
        "MONGO_URL": {
            "description": "mongo db url here",
            "required": true
        },
        "API_ID": {
            "description": "telegram api id here",
            "required": true
        },
        "API_HASH": {
            "description": "telegram api hash here",
            "required": true
        }
    },
    "addons": [
        {
            "plan": "heroku-postgresql"
        }
    ]
}
